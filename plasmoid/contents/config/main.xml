    </entry>
    <entry name="labelShadowVoff" type="int">
        <default>0</default>
        <label></label>
    </entry>

    <entry name="lineWidth" type="int">
        <default>1</default>
        <label></label>
    </entry>
    <entry name="lineColor" type="color">
        <default>#fcfcfc</default>
        <label></label>
    </entry>
