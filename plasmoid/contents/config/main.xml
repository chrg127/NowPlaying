        <default>#181717</default>
        <label></label>
    </entry>
    <entry name="secondShadowRadius" type="int">
        <default>0</default>
        <label></label>
    </entry>
    <entry name="secondShadowHoff" type="int">
        <default>0</default>
        <label></label>
    </entry>
    <entry name="secondShadowVoff" type="int">
        <default>0</default>
        <label></label>
    </entry>

    <entry name="labelFont" type="string">
        <default>default</default>
        <label></label>
    </entry>
    <entry name="labelFontColor" type="color" >
        <default>#181717</default>
        <label></label>
    </entry>
    <entry name="labelFontBold" type="bool"  >
        <default>true</default>
        <label></label>
